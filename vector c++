#include <iostream>
#include<vector>

using namespace  std;
int main(){
    vector<int>v;
    cout<<"capacity"<<v.capacity()<<endl;
    v.push_back(1);
    cout<<"capacity"<<v.capacity()<<endl;
    cout<<"Before pop"<<endl;
    for(int i:v){
        cout<<"i"<<" ";
    }cout<<endl;
    v.pop_back();
    cout<<"after pop"<<endl;
    for(int i:v){
        cout<<i<<" ";
    }
    return 0;
}
